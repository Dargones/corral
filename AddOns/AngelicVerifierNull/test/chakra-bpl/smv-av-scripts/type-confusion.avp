GlobalDeclarations
{
var {:propertyMap} __dType : [int]int; //{0:generic, 1:int, 2:float}
var __oldType : int; 
var __newType : int;
procedure __nondet_havoc_repl() returns (x:int); //replacement for havoc

procedure boogie_si_record_li2bpl_int(x:int); 

}

